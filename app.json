    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ranasami06.fattyfog"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
